<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="460.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Manufacturer.ManufacturerSceneController">
   <children>
      <Label layoutX="261.0" layoutY="25.0" text="Manufacturer Window">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="AddCarBtn" layoutX="417.0" layoutY="305.0" mnemonicParsing="false" onAction="#addCar" text="Add a car">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="DeleteCarBtn" layoutX="87.0" layoutY="305.0" mnemonicParsing="false" onAction="#deleteCar" text="Delete a car">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="viewAllCarBtn" layoutX="85.0" layoutY="88.0" mnemonicParsing="false" onAction="#viewAllCars" text="View all cars">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="EditCarBtn" layoutX="85.0" layoutY="130.0" mnemonicParsing="false" onAction="#editCar" text="Edit a car">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="87.0" layoutY="240.0" text="Delete car by Registration Number" />
      <TextField fx:id="DeleteCarField" layoutX="85.0" layoutY="264.0" />
   </children>
</AnchorPane>
